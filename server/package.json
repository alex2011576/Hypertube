{
	"name": "hypertube",
	"version": "1.0.0",
	"description": "",
	"main": "index.ts",
	"scripts": {
		"start": "NODE_ENV=production node src/index.js",
		"dev": "NODE_ENV=development ts-node-dev src/index.ts",
		"test": "NODE_ENV=test jest --verbose --runInBand --forceExit",
		"migration:create": "db-migrate create new-schema",
		"migration:update": "db-migrate down && db-migrate up",
		"migration:update:test": "db-migrate down -e test && db-migrate up -e test",
		"migrate-up": "db-migrate up",
		"migrate-up:test": "db-migrate up -e test",
		"migrate-up:production": "db-migrate up -e production",
		"migrate-down": "db-migrate down",
		"migrate-down:test": "db-migrate down -e test",
		"migrate-down:production": "db-migrate down -e production",
		"prettier": "prettier --config .prettierrc '**/*.ts' --write",
		"lint": "eslint --ext .ts .",
		"tsc": "tsc"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.12",
